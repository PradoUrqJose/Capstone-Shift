{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/sportcenter-shift-manager/browser"
      }
    }
  ],
  "routes": [
    {
      "src": "/(assets|main-.*|polyfills-.*|scripts-.*|styles-.*)\\.(js|css)",
      "dest": "/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
